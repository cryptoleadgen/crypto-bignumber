{
  "name": "crypto-bignumber",
  "description": "A library for arbitrary-precision decimal and non-decimal arithmetic in crypto",
  "version": "1.2.6",
  "keywords": [
    "arbitrary",
    "precision",
    "arithmetic",
    "big",
    "number",
    "decimal",
    "float",
    "biginteger",
    "bigdecimal",
    "bignumber",
    "bigint",
    "bignum"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cryptoleadgen/crypto-bignumber.git"
  },
  "main": "bignumber",
  "module": "bignumber.mjs",
  "browser": "bignumber.js",
  "types": "bignumber.d.ts",
  "exports": {
    ".": {
      "types": "./bignumber.d.ts",
      "require": "./bignumber.js",
      "import": "./bignumber.mjs",
      "browser": "./bignumber.js"
    },
    "./bignumber.mjs": "./bignumber.mjs",
    "./bignumber.js": "./bignumber.js",
    "./package.json": "./package.json"
  },
  "author": "Zachary Lawson <zlawson0102@gmail.com>",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "scripts": {
    "test": "node test/test"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/cryptoleadgen/crypto-bignumber/issues"
  },
  "homepage": "https://github.com/cryptoleadgen/crypto-bignumber#readme"
}
